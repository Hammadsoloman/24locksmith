{
  "format": "custom",
  "id": "settings",
  "label": "Settings",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "site_name": {
        "type": "Text",
        "config": {
          "label": "Site Name",
          "placeholder": ""
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": ""
        }
      },
      "fallback_og_image": {
        "type": "Image",
        "config": {
          "label": "Fallback OG Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "Header": {
      "logo": {
        "type": "Image",
        "config": {
          "label": "Logo",
          "constraint": {},
          "thumbnails": []
        }
      },
      "cta": {
        "type": "Group",
        "config": {
          "label": "CTA",
          "repeat": true,
          "fields": {
            "icon": {
              "type": "Image",
              "config": {
                "label": "Icon",
                "constraint": {},
                "thumbnails": []
              }
            },
            "label": {
              "type": "Text",
              "config": {
                "label": "Label",
                "placeholder": ""
              }
            },
            "link": {
              "type": "Link",
              "config": {
                "label": "Link",
                "placeholder": "",
                "select": null
              }
            }
          }
        }
      }
    },
    "Footer": {
      "newsletter_text": {
        "type": "StructuredText",
        "config": {
          "label": "Newsletter Text",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "menu": {
        "type": "Group",
        "config": {
          "label": "Menu",
          "repeat": true,
          "fields": {
            "link": {
              "type": "Link",
              "config": {
                "label": "link",
                "select": "document",
                "customtypes": ["page"]
              }
            }
          }
        }
      }
    },
    "Social Links": {
      "social_links": {
        "type": "Group",
        "config": {
          "label": "Social Links",
          "repeat": true,
          "fields": {
            "icon": {
              "type": "Select",
              "config": {
                "label": "Icon",
                "placeholder": "",
                "options": [
                  "Facebook",
                  "Instagram",
                  "Linkedin",
                  "Twitter",
                  "Youtube"
                ]
              }
            },
            "link": {
              "type": "Link",
              "config": {
                "label": "link",
                "placeholder": "",
                "select": null
              }
            }
          }
        }
      }
    },
    "Contact": {
      "address": {
        "type": "Text",
        "config": {
          "label": "Address",
          "placeholder": ""
