      "store_hours": {
        "type": "Text",
        "config": {
          "label": "Store Hours",
          "placeholder": ""
        }
      },
      "calendly_link": {
        "type": "Link",
        "config": {
          "label": "Calendly link",
          "placeholder": "",
          "select": null
        }
