{
  "format": "page",
  "id": "service_page",
  "label": "Service",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "who_we_are": {
              "type": "SharedSlice"
            },
            "why_choose_us": {
              "type": "SharedSlice"
            },
            "our_team": {
              "type": "SharedSlice"
            },
            "our_value": {
              "type": "SharedSlice"
            },
            "reviews": {
              "type": "SharedSlice"
            },
            "map": {
              "type": "SharedSlice"
            },
            "call_to_action": {
              "type": "SharedSlice"
            },
            "blog_grid": {
              "type": "SharedSlice"
            },
            "highlight": {
              "type": "SharedSlice"
            },
            "post": {
              "type": "SharedSlice"
            },
            "hero": {
              "type": "SharedSlice"
            },
            "video_banner": {
              "type": "SharedSlice"
            },
            "get_in_touch_block": {
              "type": "SharedSlice"
            },
            "blog_preview": {
              "type": "SharedSlice"
            },
            "testimonials": {
              "type": "SharedSlice"
            },
            "content_grid": {
              "type": "SharedSlice"
            },
            "body": {
              "type": "SharedSlice"
            },
            "banner": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
